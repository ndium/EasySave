<Window x:Class="EasySaveV2.View.LoadingBar"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:EasySaveV2.View"
        xmlns:vm="clr-namespace:EasySave.View_Model" d:DataContext="{d:DesignInstance Type=vm:CopyViewModel}"
        mc:Ignorable="d" 
         d:DesignHeight="700" d:DesignWidth="770"
        Title="LoadingBar" Height="700" Width="770">
    <Grid Background="#14213d">
        <ProgressBar  Minimum="0" Maximum="100"  x:Name="copyProgressBar" Width="500" Height="20"/>
        <TextBlock x:Name="numberProgressBar" Text="" Background="Transparent" Width="20" Height="20" Margin="651,332,99,332">
            
        </TextBlock>
    </Grid>
    
</Window>
